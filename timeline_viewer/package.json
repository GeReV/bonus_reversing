{
  "name": "vite-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn run server:dev",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/server.ts",
    "server:build": "tsc --project tsconfig.server.json",
    "client:build": "vite build",
    "storybook:build": "build-storybook -o dist/storybook",
    "build": "yarn run server:build && yarn run client:build && yarn run storybook:build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "tsc && prettier --check . && eslint . && stylelint \"**/*.css\"",
    "storybook": "start-storybook -p 6006",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.30",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.16",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-react": "^1.3.1",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "2.6.2",
    "storybook-builder-vite": "^0.1.23",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4",
    "vite": "^2.9.6"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-use": "^17.3.2",
    "react-window": "^1.8.7"
  }
}
